{
  "name": "eleventy-webmentions",
  "version": "0.1.0",
  "description": "A fork of eleventy-base-blog including webmention.io support",
  "scripts": {
    "watch": "npx eleventy --watch",
    "watch:sass": "sass --watch _src/stylesheets:_site/stylesheets",
    "build:sass": "sass _src/stylesheets:_site/_site/stylesheets",
    "watch:eleventy": "eleventy --serve",
    "build:eleventy": "eleventy",
    "start": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:*",
    "debug": "DEBUG=* npx eleventy"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/11ty/eleventy-base-blog.git"
  },
