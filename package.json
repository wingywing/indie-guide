    "url": "https://github.com/maxboeck/eleventy-webmentions/issues"
  },
  "homepage": "https://github.com/maxboeck/eleventy-webmentions#readme",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-plugin-rss": "^1.0.3",
    "@11ty/eleventy-plugin-syntaxhighlight": "^2.0.0",
    "autoprefixer": "^10.4.19",
    "dotenv": "^6.2.0",
    "lodash": "^4.17.11",
    "luxon": "^1.0.0",
    "markdown-it": "^8.4.1",
    "markdown-it-anchor": "^5.0.2",
    "node-fetch": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "sanitize-html": "^1.20.0",
    "sass": "^1.77.2"
  },
  "dependencies": {
    "@11ty/eleventy-plugin-webc": "^0.11.2",
    "@11ty/eleventy-upgrade-help": "^2.0.5",
    "eleventy-sass": "^2.2.4"
  }
