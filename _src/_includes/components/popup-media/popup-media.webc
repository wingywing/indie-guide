<popup-media>
    <a class="popup-media__link" :href="href"></a>
    <div class="popup-media__topbar">
        <slot class="popup-media__attribution" name="attribution"></slot>
        <object type="image/svg+xml" data="../../../assets/img/popup-media-buttons.svg"></object>
    </div>
    <div class="popup-media__media">
        <slot class="popup-media__media" name="media"></slot>
    </div>
</popup-media>